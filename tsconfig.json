{
  "compilerOptions": {
    "target": "es6" ,
    "module": "commonjs" ,
    "lib": ["ES6", "DOM"],
    "outDir": "./build" ,

    "strict": true ,
    "noImplicitAny": true ,

    "esModuleInterop": true,
    "skipLibCheck": true ,
    "forceConsistentCasingInFileNames": true 
  },
  "exclude": ["node_modules", "dist", "spec","images"]
}
